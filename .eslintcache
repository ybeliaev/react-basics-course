           ></Textarea>\r\n                <Footer>\r\n                    <Button onClick={handleClick}>Tweet</Button>\r\n                    <small style={{fontSize:\"14px\"}}>{MAXLEHGTHSTRING - text.length}</small>\r\n                </Footer>\r\n            </Wrap>\r\n        </div>    \r\n    )\r\n}","G:\\Projects\\react_basics_course\\src\\lessons\\8-bmi-culculator\\index.js",[],"G:\\Projects\\react_basics_course\\src\\lessons\\9-norm-weight-calulate\\index.js",["132","133"],"import React, { useState } from \"react\";\r\nimport PT from \"prop-types\";\r\n\r\n// При 1 степени ожирения вес превышает идеальный на 29%,\r\n// при второй – на 30–49%,\r\n// при третьей – на 50–99%, при четвертой (морбидное) – на 100% и больше\r\n\r\nfunction calculateIndex(weight, height, gender = \"Male\") {\r\n  if (gender == \"Female\") {\r\n    return 100 - ((50 + (height - 150) * 0.75 - 3.5) * 100) / weight;\r\n  }\r\n  return 100 - ((50 + (height - 150) * 0.75) * 100) / weight;\r\n}\r\n\r\nfunction tellIndex(idx) {\r\n  return idx <= 29\r\n    ? \"Нет ожирения\"\r\n    : idx <= 49\r\n    ? \"1-я степень\"\r\n    : idx <= 59\r\n    ? \"2-я степень\"\r\n    : \"морбидное\";\r\n}\r\nfunction tellIndexColor(idx) {\r\n  return idx <= 29\r\n    ? \"success\"\r\n    : idx <= 49\r\n    ? \"warning\"\r\n    : idx <= 59\r\n    ? \"danger\"\r\n    : \"danger\";\r\n}\r\n\r\nfunction RangeInput({ min, max, value, title, onChange }) {\r\n  return (\r\n    <div>\r\n      <label>\r\n        <strong>{title}</strong>\r\n        <br />\r\n        <input\r\n          type=\"range\"\r\n          min={min}\r\n          max={max}\r\n          value={value}\r\n          onChange={(e) => onChange(parseInt(e.target.value))}\r\n          style={{ width: \"15rem\" }}\r\n        />{\" \"}\r\n        {value}\r\n      </label>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default function Lesson9() {\r\n  let [gender, setGender] = useState(\"Male\");\r\n  let [height, setHeight] = useState(175);\r\n  let [weight, setWeight] = useState(75);\r\n\r\n  let resultIndex = calculateIndex(weight, height, gender);\r\n  let indexText = tellIndex(resultIndex);\r\n  let indexColor = tellIndexColor(resultIndex);\r\n  console.log(resultIndex);\r\n\r\n  return (\r\n    <div className=\"wrapper_lesson\">\r\n      <h3>Урок 9</h3>\r\n      <h4>Расчёт степени ожирения</h4>\r\n      <RangeInput\r\n        min={150}\r\n        max={200}\r\n        value={height}\r\n        title=\"Height, cm\"\r\n        onChange={setHeight}\r\n      />\r\n      <RangeInput\r\n        min={50}\r\n        max={180}\r\n        value={weight}\r\n        title=\"Weight, kg\"\r\n        onChange={setWeight}\r\n      />\r\n      <div onChange={(e) => setGender(e.target.value)}>\r\n        <div className=\"form-check form-check-inline\">\r\n          <input\r\n            className=\"form-check-input\"\r\n            type=\"radio\"\r\n            name=\"gender\"\r\n            id=\"flexRadioDefault1\"\r\n            value=\"Male\"\r\n            checked={gender === \"Male\"}\r\n          />\r\n          <label className=\"form-check-label\" htmlFor=\"flexRadioDefault1\">\r\n            <span>&#128104;</span>\r\n          </label>\r\n        </div>\r\n        <div className=\"form-check form-check-inline\">\r\n          <input\r\n            className=\"form-check-input\"\r\n            type=\"radio\"\r\n            name=\"gender\"\r\n            id=\"flexRadioDefault2\"\r\n            value=\"Female\"\r\n            checked={gender === \"Female\"}\r\n          />\r\n          <label className=\"form-check-label\" htmlFor=\"flexRadioDefault2\">\r\n            <span>&#128105;</span>\r\n          </label>\r\n        </div>\r\n        My gender is {gender.toLowerCase()}\r\n      </div>\r\n      <hr />\r\n      <div className=\"mt-2\">\r\n        BMI: <strong>{resultIndex.toFixed(1)}</strong>{\" \"}\r\n        <span className={`badge bg-${indexColor}`}>{indexText}</span>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n// Warning: You provided a `checked` prop to a form field without an `onChange` handler. This will render a read-only field. If the field should be mutable use `defaultChecked`. Otherwise, set either `onChange` or `readOnly`.\r\n","G:\\Projects\\react_basics_course\\src\\lessons\\10-project_RGB-slider\\index.js",["134","135","136"],"import React, { useState } from \"react\";\r\n\r\n// Converting RGB to hex\r\nfunction RGBToHex(r, g, b) {\r\n  r = r.toString(16);\r\n  g = g.toString(16);\r\n  b = b.toString(16);\r\n\r\n  if (r.length == 1) r = \"0\" + r;\r\n  if (g.length == 1) g = \"0\" + g;\r\n  if (b.length == 1) b = \"0\" + b;\r\n\r\n  return \"#\" + r + g + b;\r\n}\r\n// **************************\r\n\r\nfunction Slider({ min, max, value, title, onChange }) {\r\n  return (\r\n    <div className=\"col-sm-4\">\r\n      <span>{title}</span>\r\n      <label className=\"form-label d-flex align-items-center\">\r\n        <input\r\n          className=\"form-range me-2\"\r\n          type=\"range\"\r\n          min={min}\r\n          max={max}\r\n          value={value}\r\n          onChange={(e) => onChange(parseInt(e.target.value))}\r\n        />\r\n        <span>{value}</span>\r\n      </label>\r\n    </div>\r\n  );\r\n}\r\nfunction ColorWrapper({ valueR, valueG, valueB }) {\r\n  return (\r\n    <div\r\n      className=\"border mt-3\"\r\n      style={{\r\n        height: \"200px\",\r\n        width: \"200px\",\r\n        backgroundColor: `rgb(${valueR}, ${valueG}, ${valueB})`,\r\n      }}\r\n    ></div>\r\n  );\r\n}\r\nfunction Badge({ valueR, valueG, valueB }) {\r\n  return (\r\n    <div>\r\n      DEC:{\" \"}\r\n      <span className=\"badge bg-dark text-monospace\">\r\n        {`rgba(${valueR},${valueG},${valueB},1)`}\r\n      </span>\r\n      <br />\r\n      HEX:{\" \"}\r\n      <span className=\"badge bg-dark text-monospace\">\r\n        {RGBToHex(valueR, valueG, valueB)}\r\n      </span>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default function Lesson10() {\r\n  let [valueR, setValueR] = useState(120);\r\n  let [valueG, setValueG] = useState(160);\r\n  let [valueB, setValueB] = useState(100);\r\n  return (\r\n    <div className=\"wrapper_lesson\">\r\n      <h3>Урок 10</h3>\r\n      <h4>Проект Color chooser</h4>\r\n      <Slider\r\n        min={0}\r\n        max={255}\r\n        title={\"R\"}\r\n        value={valueR}\r\n        onChange={setValueR}\r\n      />\r\n      <Slider\r\n        min={0}\r\n        max={255}\r\n        title={\"G\"}\r\n        value={valueG}\r\n        onChange={setValueG}\r\n      />\r\n      <Slider\r\n        min={0}\r\n        max={255}\r\n        title={\"B\"}\r\n        value={valueB}\r\n        onChange={setValueB}\r\n      />\r\n      <ColorWrapper valueR={valueR} valueG={valueG} valueB={valueB} />\r\n      <Badge valueR={valueR} valueG={valueG} valueB={valueB} />\r\n    </div>\r\n  );\r\n}\r\n","G:\\Projects\\react_basics_course\\src\\lessons\\11-TODO-APP-1\\index.js",["137","138"],"import React, { useState } from \"react\";\r\nimport TodoForm from \"./TodoForm\";\r\nimport TodoList from \"./TodoList\";\r\n\r\nconst initialTodos = [\r\n  { text: \"To study React\", done: false },\r\n  { text: \"To read Bible\", done: true },\r\n  { text: \"To do fitness\", done: false },\r\n];\r\n\r\nexport default function Lesson11() {\r\n  let [todos, setTodos] = useState(initialTodos);\r\n\r\n  function createTodo({ text }) {\r\n    let todo = { text, done: false };\r\n    return setTodos([...todos, todo]);\r\n  }\r\n  function removeTodo(text) {\r\n    setTodos(todos.filter((todo) => todo.text != text));\r\n  }\r\n  function toggleTodo(text) {\r\n    setTodos(\r\n      todos.map((todo) => ({\r\n        ...todo,\r\n        done: todo.text == text ? !todo.done : todo.done,\r\n      }))\r\n    );\r\n  }\r\n\r\n  return (\r\n    <div className=\"wrapper_lesson\">\r\n      <h3>Урок 11</h3>\r\n      <h4>TODO-APP-1</h4>\r\n      <p className=\"fs-5 text-center\">Ivan's variant</p>\r\n      <TodoForm createTodo={createTodo} />\r\n      <TodoList todos={todos} removeTodo={removeTodo} toggleTodo={toggleTodo} />\r\n    </div>\r\n  );\r\n}\r\n","G:\\Projects\\react_basics_course\\src\\lessons\\11-TODO-APP-1\\TodoForm.js",[],"G:\\Projects\\react_basics_course\\src\\lessons\\11-TODO-APP-1\\TodoList.js",[],"G:\\Projects\\react_basics_course\\src\\lessons\\11-TODO-APP-1\\TodoItem.js",[],"G:\\Projects\\react_basics_course\\src\\lessons\\11-TODO-APP-1\\Footer.js",["139","140","141"],"import React from \"react\";\r\n\r\nimport { FILTER_ACTIVE, FILTER_ALL, FILTER_DONE } from \"./TodoList\";\r\n\r\nexport default function Footer({ filter, setFilter }) {\r\n  return (\r\n    <div className=\"fs-5\">\r\n      <span>Show: </span>\r\n      {filter == FILTER_ALL ? (\r\n        <a href=\"#all\">\r\n          <b>All</b>\r\n        </a>\r\n      ) : (\r\n        <a\r\n          href=\"#all\"\r\n          onClick={(e) => {\r\n            e.preventDefault();\r\n            setFilter(FILTER_ALL);\r\n          }}\r\n        >\r\n          All\r\n        </a>\r\n      )}\r\n      {\", \"}\r\n      {filter == FILTER_ACTIVE ? (\r\n        <a href=\"#active\">\r\n          <b>Active</b>\r\n        </a>\r\n      ) : (\r\n        <a\r\n          href=\"#active\"\r\n          onClick={(e) => {\r\n            e.preventDefault();\r\n            setFilter(FILTER_ACTIVE);\r\n          }}\r\n        >\r\n          Active\r\n        </a>\r\n      )}\r\n      {\", \"}\r\n      {filter == FILTER_DONE ? (\r\n        <a href=\"#done\">\r\n          <b>Done</b>\r\n        </a>\r\n      ) : (\r\n        <a\r\n          href=\"#done\"\r\n          onClick={(e) => {\r\n            e.preventDefault();\r\n            setFilter(FILTER_DONE);\r\n          }}\r\n        >\r\n          Done\r\n        </a>\r\n      )}\r\n    </div>\r\n  );\r\n}\r\n","G:\\Projects\\react_basics_course\\src\\lessons\\12-calories-app\\index.tsx",[],["142","143"],"G:\\Projects\\react_basics_course\\src\\lessons\\12-calories-app\\CaloriesTodoSearch